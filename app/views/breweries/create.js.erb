$("#beer").html("<%= j render partial: 'brewery', locals: {brewery: @brewery, chosen_beer: @chosen_beer} %>")

var lat = <%= j @brewery.latitude.to_s %>;
var lng = <%= j @brewery.longitude.to_s %>;

var start_lat = <%= j params[:brewery][:latitude] %>;
var start_lng = <%= j params[:brewery][:longitude] %>;

var map = new GoogleMap();
map.initialize(start_lat, start_lng);
map.addMaptoPage();
map.calcRoute(lat, lng);



